{
    "name": "black-knight",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@chakra-ui/icons": "^2",
        "@chakra-ui/react": "^2",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "axios": "^0.22.0",
        "body-parser": "^1.19.0",
        "classnames": "^2.3.1",
        "concurrently": "^6.3.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "formik": "^2.2.9",
        "framer-motion": "^4",
        "fuzzysort": "^2.0.1",
        "immer": "^9.0.6",
        "lodash": "^4.17.21",
        "mana-font": "^1.14.0",
        "mongodb": "^4.1.3",
        "mongoose": "^6.0.12",
        "morgan": "^1.10.0",
        "nanoid": "^4.0.0",
        "nodemon": "^2.0.13",
        "prop-types": "^15.8.1",
        "qs": "^6.11.0",
        "react": "^18",
        "react-dom": "^18",
        "react-horizontal-scrolling-menu": "^2.5.1",
        "react-icons": "^4.3.1",
        "react-query": "^3.29.0",
        "react-scripts": "4.0.3",
        "recoil": "^0.7.3",
        "typescript": "^4.4.4",
        "web-vitals": "^1.1.2",
        "windows-1250": "^3.0.2"
    },
    "scripts": {
        "start": "concurrently \"yarn run backend\" \"yarn run client\"",
        "client": "PORT=3000 react-scripts start",
        "backend-nodemon": "PORT=3001 nodemon backend-wrapper.js",
        "backend": "PORT=3001 node backend-wrapper.js",
        "debug-scraper": "node testing/debugScraper-wrapper.js",
        "try": "node -r esm testing/try.js",
        "try_autocomplete": "node -r esm testing/try_autocomplete.js",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix --ext .js,.jsx ."
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "node": true
        },
        "extends": [
            "react-app",
            "react-app/jest",
            "eslint:recommended",
            "plugin:react/recommended",
            "plugin:react/jsx-runtime"
        ],
        "parserOptions": {
            "ecmaFeatures": {
                "jsx": true,
                "modules": true
            },
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "plugins": [
            "react",
            "unused-imports"
        ],
        "rules": {
            "unused-imports/no-unused-imports": "warn",
            "react/prop-types": [
                "warn",
                {
                    "ignore": [
                        "children"
                    ]
                }
            ],
            "no-unused-vars": "warn",
            "semi": [
                2,
                "never"
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:3001",
    "devDependencies": {
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "react-error-overlay": "6.0.9"
    }
}